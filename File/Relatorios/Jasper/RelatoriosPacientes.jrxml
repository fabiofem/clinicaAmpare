	<queryString language="SQL">
		<![CDATA[SELECT clinica.passagem.`dataEntrada` AS DataEntrada,
	clinica.passagem.entrada AS Entrada,
	clinica.paciente.`nomeCompleto` AS NomeCompleto
FROM clinica.passagem
	INNER JOIN clinica.paciente ON 
	 clinica.paciente.id = clinica.passagem.pessoa_id 
